<%= simple_form_for([@station,@tank])do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :type_of_fuel %>
    <%= f.input :size %>
    <%= f.input :registration_id %>
    <%= f.association :station %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
